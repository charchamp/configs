  "packageManager": "^pnpm@7.13.0",
  "description": "personal configs",
  "keywords": [
    "pnpm",
    "monorepo",
    "configs",
    "prettier"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sawndev/configs#readme",
  "bugs": "https://github.com/sawndev/configs/issues",
  "repository": {
